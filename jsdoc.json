{
  "plugins": ["plugins/markdown"],

  "source": {
    "include": [
      "src/",
      "src/HDInterface",
      "src/digiSign",
      "src/digiSign",
      "test",
      "test/fixtures",
      "test/integration"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/"
  }
}
